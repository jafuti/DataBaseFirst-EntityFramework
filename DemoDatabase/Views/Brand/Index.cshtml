@model List<DemoDatabase.Models.Brand>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Brand</h2>
<table class="table">
    <thead>
        <tr>
            <th>Brand ID</th>
            <th>Brand Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cat in Model)
        {
            <tr>
        <td>@cat.BrandID</td>
        <td>@cat.BrandName</td>
                </tr>
    }
        </tbody>
    </table>

