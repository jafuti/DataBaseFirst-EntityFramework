@model List<DemoDatabase.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Category</h2>
<table class="table">
    <thead>
        <tr>
            <th>Category ID</th>
            <th>Category Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var cat in Model)
            {
                <td>@cat.CategoryID</td>
                <td>@cat.CategoryName</td>
            }
    </tbody>
</table>
